{
	"storage": {
		"gqllambdadb": {
			"service": "DynamoDB",
			"providerPlugin": "awscloudformation"
		},
		"currencytable": {
			"service": "DynamoDB",
			"providerPlugin": "awscloudformation"
		}
	},
	"function": {
		"currencyfunction": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "storage",
					"resourceName": "currencytable",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		}
	},
	"api": {
		"gqllambdacrypto": {
			"service": "AppSync",
			"providerPlugin": "awscloudformation",
			"output": {
				"securityType": "API_KEY"
			}
		}
	}
}